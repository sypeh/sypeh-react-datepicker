{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "jsx": "react",
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    }
}